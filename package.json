{
  "name": "site-list",
  "version": "0.0.1",
  "description": "A generator for a website that shows a list of sites with some common features. e.g. top javascript frameworks",
  "productName": "Site List",
  "author": "Dan Bar-Shalom <dan@jolly.tools>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:ssg": "quasar ssg generate",
    "dev:ssg": "quasar ssg dev",
    "serve:ssg": "quasar ssg serve dist/ssg"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "eta": "^3.4.0",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "v-shared-element": "^3.1.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "quasar-app-extension-ssg": "^5.1.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}